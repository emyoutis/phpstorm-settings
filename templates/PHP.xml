<templateSet group="PHP">
  <template name="mdd" value="dd($END$, __FILE__ . ' - ' . __LINE__);" description="dd with position info" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="'" value="'$END$'" description="single quote" toReformat="false" toShortenFQNames="true" />
  <template name="dad" value="dump($END$, __FILE__ . ' - ' . __LINE__);&#10;die(__FILE__ . ' - ' . __LINE__);" description="dump and die with position info" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="pubf" value="public function $NAME$($PARAMETERS$)$RETURN${&#10;    $END$&#10;}" description="public function" toReformat="true" toShortenFQNames="true">
